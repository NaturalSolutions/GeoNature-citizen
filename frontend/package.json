{
    "name": "frontend",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start:fr": "ng serve --poll 2000 --configuration=fr",
        "start:en": "ng serve --poll 2000 --configuration=en",
        "build": "ng build",
        "build:fr": "ng build --configuration=fr",
        "build:en": "ng build --configuration=en",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "compile:server": "webpack --config webpack.server.config.js --progress --colors",
        "serve:ssr": "node dist/server",
        "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
        "build:i18n-ssr": "npm run build:client-and-server-bundles-i18n && npm run compile:server",
        "build:client-and-server-bundles": "ng build --prod && ng run frontend:server:production",
        "build:client-and-server-bundles-i18n": "ng run frontend:build:fr && ng run frontend:build:en && ng run frontend:server:production",
        "extract-i18n": "ng xi18n frontend --i18n-format xlf --output-path i18n --i18n-locale fr && ng run frontend:xliffmerge"
    },
    "private": true,
    "dependencies": {
        "@ajsf/bootstrap4": "^0.1.4",
        "@angular/animations": "^8.2.2",
        "@angular/cdk": "^8.2.3",
        "@angular/common": "^8.2.2",
        "@angular/compiler": "^8.2.2",
        "@angular/core": "^8.2.2",
        "@angular/flex-layout": "^6.0.0-beta.18",
        "@angular/forms": "^8.2.2",
        "@angular/http": "^7.2.15",
        "@angular/material": "^7.3.3",
        "@angular/platform-browser": "^8.2.2",
        "@angular/platform-browser-dynamic": "^8.2.2",
        "@angular/platform-server": "^8.2.2",
        "@angular/router": "^8.2.2",
        "@ng-bootstrap/ng-bootstrap": "^5.1.0",
        "@nguniversal/express-engine": "^8.1.1",
        "@nguniversal/module-map-ngfactory-loader": "8.1.1",
        "@ngx-i18nsupport/tooling": "^8.0.3",
        "@swimlane/ngx-datatable": "^16.0.2",
        "angular2-image-upload": "^1.0.0-rc.2",
        "bootstrap": "^4.3.1",
        "core-js": "^3.2.1",
        "domino": "^2.1.3",
        "express": "^4.17.1",
        "file-saver": "^2.0.5",
        "font-awesome": "^4.7.0",
        "jquery": "^3.4.1",
        "jshint": "^2.10.2",
        "leaflet": "^1.5.1",
        "leaflet-fullscreen": "^1.0.2",
        "leaflet-gesture-handling": "^1.1.8",
        "leaflet.locatecontrol": "^0.68.0",
        "leaflet.markercluster": "^1.4.1",
        "lodash": "^4.17.15",
        "lodash-es": "^4.17.15",
        "mock-browser": "^0.92.14",
        "ngx-toastr": "^10.1.0",
        "rxjs": "~6.5.2",
        "uninstall": "0.0.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.803.21",
        "@angular/cli": "^8.3.21",
        "@angular/compiler-cli": "^8.2.2",
        "@angular/language-service": "^8.2.2",
        "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
        "@types/jasmine": "~3.4.0",
        "@types/jasminewd2": "~2.0.6",
        "@types/leaflet": "^1.5.1",
        "@types/leaflet.locatecontrol": "^0.60.7",
        "@types/leaflet.markercluster": "^1.4.2",
        "@types/node": "~13.1.4",
        "@typescript-eslint/eslint-plugin": "^4.14.2",
        "@typescript-eslint/parser": "^4.14.2",
        "codelyzer": "~5.1.0",
        "eslint": "^7.19.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-prettier": "^3.3.1",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.2.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "prettier": "^2.2.1",
        "protractor": "~5.4.2",
        "ts-loader": "^6.0.4",
        "ts-node": "~8.3.0",
        "tslint": "~5.18.0",
        "typescript": "^3.5.3",
        "webpack-cli": "^3.3.7"
    }
}
